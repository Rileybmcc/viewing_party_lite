<h2>Create New User</h2>
<%= form_with url: "/users", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <br>
  <%= f.label :password %>
  <%= f.text_field :password %><br>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  <br>
  <br>
  <%= f.submit :'Create New User' %>
<% end %>
